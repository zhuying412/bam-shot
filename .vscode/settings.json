{
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.formatting.yapfArgs": [
    "--style",
    "{BASE_ON_STYLE = pep8, COLUMN = 248}"
  ],
  "python.linting.flake8Args": ["--max-line-length=248"],
  "editor.formatOnSave": true,
  "cSpell.words": [
    "chrom",
    "Fasta",
    "figsize",
    "hlines",
    "samtools",
    "savefig",
    "tview",
    "xlim",
    "xticks",
    "yaxis",
    "ylim",
    "ynames",
    "yticks"
  ]
}
